digraph G {
	margin="0"
	root[shape=circle, label=""];
	front[shape=record, label=" f | e "];
	mid[shape=circle, label=""];
	rear[shape=record, label="a"];
	root->front;
	root->mid;
	root->rear;
	{rank=same front->mid->rear[style=invis]}
	
	br3[shape=point, label="branch-3"];
	d[shape=circle];
	c[shape=circle];
	b[shape=circle];
	mid->br3;
	br3->d;
	br3->c;
	br3->b;
	{rank=same d->c->b[style=invis]}
}
