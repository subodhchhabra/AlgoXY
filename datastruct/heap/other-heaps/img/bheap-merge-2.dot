digraph G{
	margin="0"
	node[shape=record]
	b1[label="<head>t1|<ts>..."];
	b2[label="<head>t2|<ts>..."];
	t[label="Rank(t1)=Rank(t2)", shape=ellipse];
	b1:head->t;
	b2:head->t;
	r1[label="T1|T2|..."];
	r2[label="+", shape=plaintext];
	ri[label="Ti"];
	rs[label="merge rest"];
	ri->rs[label="insert"];
	{rank=same r1 r2 ri rs}
	t->ri[label=" link(t1, t2)"];
	r1->r2[style=invis];
	r2->ri[style=invis];
	b1:ts->rs;
	b2:ts->rs;
}
